{
  "name": "sass-esbuild-plugin",
  "version": "0.6.0",
  "description": "sass plugin",
  "keywords": [
    "esbuild",
    "sass"
  ],
  "author": "Liu Bowen <mr_lbw@outlook.com>",
  "homepage": "https://github.com/lbwa/esw/tree/main/packages/plugin-sass#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbwa/esw.git",
    "directory": "packages/plugin-sass"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "prebuild": "yarn clean",
    "build": "yarn build:lib && yarn build:types",
    "build:lib": "esw build src/index.ts --bundle --platform=node --tsconfig=tsconfig.build.json",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "bugs": {
    "url": "https://github.com/lbwa/esw/issues"
  },
  "peerDependencies": {
    "esbuild": "^0.14.10",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@lbwa/tsconfig": "^1.1.0",
    "@types/sass": "^1.16.1",
    "esw": "^0.13.0",
    "sass": "^1.35.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@eswjs/common": "^0.7.0"
  }
}
